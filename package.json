{
  "name": "goole-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:clean": "npm run clean && next dev",
    "dev:force": "npm run clean:all && next dev",
    "dev:smart": "node scripts/smart-dev.js",
    "build": "next build",
    "build:clean": "npm run clean && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next",
    "clean:all": "rm -rf .next node_modules/.cache .swc",
    "clean:cache": "rm -rf node_modules/.cache .swc",
    "clean:script": "node scripts/clean-build.js",
    "clean:deep": "node scripts/clean-build.js --deep",
    "health-check": "node scripts/dev-health-check.js",
    "optimize-hmr": "node scripts/optimize-hmr.js",
    "reset": "npm run clean:all && npm install",
    "upload-images": "node scripts/upload-missing-images.js",
    "update-cache": "node scripts/update-cache-headers.js",
    "oss-optimize": "node scripts/oss-cache-config.js",
    "analyze": "ANALYZE=true npm run build",
    "deps:check": "npm outdated",
    "deps:update": "npm update"
  },
  "dependencies": {
    "@googlemaps/react-wrapper": "^1.1.35",
    "@heroicons/react": "^2.2.0",
    "@heroui/react": "^2.8.4",
    "@nextui-org/react": "^2.6.11",
    "@tailwindcss/postcss": "^4.1.13",
    "autoprefixer": "^10.4.21",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.22",
    "next": "15.5.3",
    "pdf-lib": "^1.17.1",
    "pdf2pic": "^3.2.0",
    "react": "^18",
    "react-dom": "^18",
    "react-photo-view": "^1.2.7",
    "sharp": "^0.33.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.13"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "ali-oss": "^6.23.0",
    "dotenv": "^17.2.2",
    "eslint": "^8",
    "eslint-config-next": "15.5.3",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
